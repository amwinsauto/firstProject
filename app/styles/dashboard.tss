"#dashboardWindow":{
	backgroundImage:"/images/bgdashboard_phone.jpg",
	orientationModes : [Ti.UI.PORTRAIT]
}
"#dashboardWindow[platform=ios]":{
	statusBarStyle: Ti.UI.iPhone.StatusBar.LIGHT_CONTENT
}
"#dashboardWindow[platform=ios formFactor=tablet]":{
	backgroundImage: '/images/bgdashboard_tablet.jpg',
	orientationModes : [Ti.UI.LANDSCAPE_LEFT, Ti.UI.LANDSCAPE_RIGHT]
}
"#dashboardWindow[platform=android]":{
	exitOnClose: Alloy.Globals.workspaceExitOnClose,
	titleid:"home_title",
	navBarHidden :false,
	backgroundImage:"/images/bgdashboard_phone.jpg"
}
"#actionbar": {
	homeButtonEnabled : true,
	icon: '/images/core/menubutton.png'
}
"#menuButton[platform=ios]": {
	backgroundImage:"",
	image:"/images/core/menubutton.png",
	top: 15,
	left: 15,
	width: 44,
	height: 44

}
"#title[platform=ios]": {
	top:20,
	left:50,
	right:50,
	height:30,
	color:"#ffffff",
	textAlign:"center",
	textid:"home_title",
	font: { fontSize: 20, fontWeight: 'bold' }
}
