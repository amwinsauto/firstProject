"#dashboardPages": {
	showPagingControl: true
}
"#dashboardPages[platform=ios]": {
	top: 85,
	pagingControlColor: "transparent",
	pagingControlHeight: 30
}
"#dashboardPages[platform=ios formFactor=tablet]": {
	top: 90
}
"#dashboardPages[platform=android]": {
	showPagingControl: false,
	top: 45
}
